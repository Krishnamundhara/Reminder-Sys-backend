# Backend-specific .gitignore

# Dependencies
/node_modules
/npm-debug.log

# Environment files
.env
# Keep example environment files for documentation
!.env.example
!.env.production

# Database credentials
*.pem
*.key
*.crt
certificates/

# Logs
logs
*.log
npm-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Directory for instrumented libs generated by jscoverage/JSCover
lib-cov

# Coverage directory used by tools like istanbul
coverage
*.lcov

# nyc test coverage
.nyc_output

# Dependency directories
node_modules/

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# Database backups
*.sql
*.dump
*.backup
/backups/

# Temporary files
tmp/
temp/
uploads/

# Local development files
*.local
*.development
.development/

# IDE specific files
.idea/
.vscode/
*.sublime-workspace
*.sublime-project
.DS_Store
Thumbs.db

# Auto-generated API documentation
/docs/api/
/docs/generated/
